{
  "name": "flow-gui",
  "displayName": "flow-gui",
  "description": "",
  "publisher": "h",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.97.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/commands/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "flow-gui.showWebView",
        "title": "showFlowGUI"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "flow-gui.showWebView",
          "when": "resourceLangId == 'plaintext'",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "npm run commands:build && npm run vite:build",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "commands:build": "tsc -p ./src/commands",
    "vite:build": "vite build",
    "vite:watch": "vite build --watch"
  },
  "devDependencies": {
    "@types/dagre": "^0.7.52",
    "@types/mocha": "^10.0.10",
    "@types/node": "^20.17.19",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@types/vscode": "^1.97.0",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.19.0",
    "typescript": "^5.7.3",
    "vite": "^6.1.0"
  },
  "dependencies": {
    "@dagrejs/dagre": "^1.1.4",
    "@types/vscode-webview": "^1.57.5",
    "@xyflow/react": "^12.4.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  }
}
